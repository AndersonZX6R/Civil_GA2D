#pragma hdrstop

#include "Civil_Math.h"
#pragma package(smart_init)

#include <math.h>

namespace Civil
{

namespace MATH
{

void
Angle::calcRadians()
{
	m_dblRadians = m_degrees + (m_minutes / 60) + (m_seconds / 3600);
	m_dblRadians = PI * (m_dblRadians / 180);
}

void
Angle::calcDegrees()
{

}

} // namespace MATH

} // namespace Civil
